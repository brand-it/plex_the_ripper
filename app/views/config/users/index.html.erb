<p id="notice"><%= notice %></p>

<h1>Config Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @config_users.each do |config_user| %>
      <tr>
        <td><%= link_to 'Show', config_user %></td>
        <td><%= link_to 'Edit', edit_config_user_path(config_user) %></td>
        <td><%= link_to 'Destroy', config_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Config User', new_config_user_path %>
